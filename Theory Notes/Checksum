 digits = []
Sum=0

digit = input("Enter a 7 digit number: ")
for c in digit:
    digits.append(int(c))


Weights= [2,7,6,5,4,3,2]

for i in range(len(digits)):
    Sum += digits[i]*Weights[i]
Sum+=4

remainder = Sum%11 

number = 11-remainder

alphabets = ['a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z']
print("The check code for",digit,'is',alphabets[number-1].upper())
